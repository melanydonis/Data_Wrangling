##Mientras no le pegue, continua probando
while(ad != numero)
{
##Que la persona puebe un número
ad <- readline(prompt = "Prueba un número: ")
if (ad == numero)
{
##Si el número es el correcto
cat(numero, "¡Correcto!")
}
else if (ad < numero)
{
##Especifica que es más grande
cat("Es más grande")
}
else if(ad > numero)
{
##Especifica que es más pequeño
cat("Es más Pequeño")
}
##Loop infinito mientras no atine
}
